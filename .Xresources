!##########################################
!#                 Colors                 #
!##########################################

! special
*.foreground:   #dfdfdf
*.background:   #070e14
*.cursorColor:  #dfdfdf

! black
*.color0:       #070e14
*.color8:       #3C4B54

! red
*.color1:       #eb1f1c
*.color9:       #eb1f1c

! green
*.color2:       #a2ba00
*.color10:      #a2ba00

! yellow
*.color3:       #ffc100
*.color11:      #ffc100

! blue
*.color4:       #0076ff
*.color12:      #0076ff

! magenta
*.color5:       #f0147f
*.color13:      #f0147f

! cyan
*.color6:       #00d7c6
*.color14:      #00d7c6

! white
*.color7:       #dfdfdf
*.color15:      #dfdfdf

!##########################################
!#                 URxvt                  #
!##########################################
URxvt.tabbed.tabbar-fg: 2
URxvt.tabbed.tabbar-bg: 0
URxvt.tabbed.tab-fg: 0
URxvt.tabbed.tab-bg: 2


URxvt.keysym.Shift-Control-V:    perl:clipboard:paste
URxvt.keysym.Shift-Control-c:    perl:clipboard:copy
URxvt.keysym.Shift-t:	         perl:tabbedex:new_tab
URxvt.keysym.Shift-Right:	     perl:tabbedex:next_tab
URxvt.keysym.Shift-Left:	     perl:tabbedex:prev_tab
URxvt.iso14755:                  False
URxvt.perl-ext-common:           default,tabbedex,matcher,clipboard,selection-to-clipboard
URxvt.url-launcher: /usr/bin/xdg-open
URxvt.matcher.button: 1
URxvt*cursorUnderline:           true
URxvt.cursorBlink:               false
URxvt*scrollBar:                 false
URxvt*scrollBar_right:           false
URxvt*transparent:               false
URxvt*depth:                     32

URxvt.letterSpace:               1

URxvt*scrollTtyOutput: false
URxvt*scrollTtyKeypress: true

!##########################################
!#                 Xterm                  #
!##########################################

! General settings
XTerm*termName:         xterm-256color
XTerm*geometry:         65x20
XTerm*saveLines:        1024
XTerm*scrollBar:        False
XTerm*loginshell:       True
XTerm*cursorBlink:      True

! Fonts and Encoding
XTerm*utf8:             True
XTerm*locale:           True
XTerm*utf8Title:        True
XTerm*renderFont:       True
XTerm*preeditType:      Root
XTerm*xftAntialias:     True
XTerm*faceName:         Fira Mono
XTerm*faceSize:         14
! Trim trailing spaces
XTerm*trimSelection:    True

! Fix alt-key inputting
XTerm*eightBitInput:    False
XTerm*metaSendsEscape:  True

! Copy and paste with clipboard
XTerm*translations: #override \
	Ctrl Shift <KeyPress> C: copy-selection(CLIPBOARD) \n\
	Ctrl Shift <KeyPress> V: insert-selection(CLIPBOARD)

! Allow selecting email/url by double-click
XTerm*charClass: 33:48,37-38:48,45-47:48,64:48,58:48,126:48,61:48,63:48,43:48,35:48

!----------------
!  Xft Settings
!----------------

Xft*dpi:                82
Xft*antialias:          True
Xft*hinting:            True
Xft*rgba:               rgb
Xft*autohint:           False
Xft*hintstyle:          hintfull
Xft*lcdfilter:          lcddefault
